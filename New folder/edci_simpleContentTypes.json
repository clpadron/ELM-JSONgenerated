            "@base": "xs:anyURI",
            "pattern": [
               {
                  "@value": "http://.*"
               },
               {
                  "@value": "https://.*"
               }
            ]
         }
      },
      {
         "@name": "MailToUriType",
         "annotation": {
            "documentation": {
               "@lang": "en",
               "#text": "mailto: URI scheme conform to RFC 2368."
            }
         },
         "restriction": {
            "@base": "xs:anyURI",
            "pattern": {
               "@value": "mailto:.*"
            }
         }
      }
   ]
}

